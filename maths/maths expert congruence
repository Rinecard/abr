def D(n):
    a=n**3-n
    r1=a%2
    r2=a%3
    print(r1,r2)
    return r1,r2 
#D(5454) D(4)
# 0
# disjonction des cas pour modulo3 cela renvoie touujours 0

def restes(n):
    u=14
    for i in range(1,n+1):
        u=5*u-6
    r=u%100
    return r ,u

#cette fonction renvoie le reste de u n divis√© par 100 
#print(restes(20),restes(21))
